#include "common.h"

struct 	v2p
{
 	float2 	tc0	: 	TEXCOORD0;	// base
	half4	c	:	COLOR0;		// diffuse
};


//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
half4 	main( v2p I )	: COLOR
{
	return	round(I.c.a*tex2D(s_base,I.tc0).a*100.f)/100.f+0.00001f;
}
